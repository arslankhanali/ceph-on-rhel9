{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ListObjectsInBucket",
            "Effect": "Allow",
            "Principal": {"AWS": ["arn:aws:iam:::user/test-view"]},
            "Action": [
                "s3:GetObject",
                "s3:GetObjectVersion",
                "s3:GetObjectTagging",
                "s3:GetObjectTorrent",
                "s3:GetObjectAcl",
                "s3:ListBucket"
            ],
            "Resource": ["arn:aws:s3:::test-bucket"]
        }
    ]
}